{
  "type": "shell",
  "command": "java",
  "windows": {
    "command": "javaw"
  },
  "args": [
    "-cp",
    "/usr/local/opt/languagetool/libexec/languagetool-server.jar",
    "org.languagetool.server.HTTPServer",
    "--port",
    "8989"
  ],
  "label": "LanguageTool: serve",
  "isBackground": true,
  "group": "none",
  "presentation": {
    "reveal": "silent",
    "panel": "dedicated",
    "showReuseMessage": false
  },
  "runOptions": {
    "runOn": "default",
    "reevaluateOnRerun": true
  }
}
